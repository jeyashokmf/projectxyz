<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Priority_change_check</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
	 ISPICKVAL(PRIORVALUE(Priority__c ), &quot;high&quot;), 
	 ISPICKVAL(Priority__c, &quot;low&quot;)
)</errorConditionFormula>
    <errorMessage>Priority can&#39;t be changed from high to low</errorMessage>
</ValidationRule>
