<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_name_and_last_name_check</fullName>
    <active>false</active>
    <errorConditionFormula>UPPER( FirstName )  =  UPPER( LastName )</errorConditionFormula>
    <errorMessage>First name and last name should not be the same</errorMessage>
</ValidationRule>
